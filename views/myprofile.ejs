<div id="searchengine" style="text-align:center">

    <h1 id="head">  <span style="margin-left: 10%;"> Profile about the <%= plants[0].common_name %> </span>    </h1>
    <img src='images\plantsimages\<%= plants[0].common_name%>.jpg' alt="">

    <div id="profileproperties" >

        <div id='leftpart'>
            <ul> 
            <p class = "hd"> basics</p>
             <li id = "french">     <span>French Name: </span><%= plants[0].frenchname %> </li> 
                <li id = "arabic">    <span>Arabic Name: </span><%= plants[0].arabicname %></li> 
                <li id = "p-type">   <span>Plant type: </span><%= plants[0].planttype %></li> 
                <li id = "origin"> <span>Origin: </span><%= plants[0].country %></li> 
               <li id = "growth"> <span>Growth Rate: </span> <%= plants[0].radio_growthRate %></li> 
                <li id = "shape"> <span>canopy Shape: </span><%= plants[0].canopyshape %> </li> 

            </ul>





            <ul> 
                <p class = "hd"> Foliage</p>  
                <li id = "leaf-per"> <span>Leaf Persistance: </span>  <%= plants[0].radio_leafPersistance %></li> 
                <li id = "color-grow"><span>Color(growing season): </span> <%= plants[0].leafcolorgrowing %></li> 
               <li id = "color-come"> <span>Color(changing season): </span> <%= plants[0].leafcolorchanging %></li> 
            </ul>  
            
            <ul>
              <p class = "hd">Flower Showiness </p> 
              <li id = "flr-sexuality"> <span>Showiness: </span><%= plants[0].radio_flowerShowiness %> </li>
              <li id = "flr-sexuality"> <span>scent: </span><%= plants[0].radio_flowerScent %> </li>
           <li id = "flr-color"> <span>Color: </span><%= plants[0].flowerColor %></li>
             <li id = "season"> <span>Seasons: </span><%= plants[0].radio_flowerSeason %></li>
          </ul>


          <ul>
              <p class = "hd">Fruit Type</p>
               <li id = "frt-show"> <span>Fruit Showiness: </span><%= plants[0].radio_fruitShowiness %></li> 
              <li id = "frt-range"> <span>Fruit type : </span><%= plants[0].radio_fruitTypeCategory %></li> 

          </ul>

          <ul> 
              <p class = "hd">Tolerance</p>
              <li id = "frost-tol"> <span>Frost Tolerance: </span><%= plants[0].radio_frostTolerance %> </li> 
              <li id = "heat-tol" > <span>heat Tolerance: </span> <%= plants[0].radio_heatTolerance %></li> 
               <li id = "dt-tel"> <span>Drought Tolerance: </span><%= plants[0].radio_droughtTolerance %> </li> 
              <li id = "salt-tel">  <span>Salt Tolerance: </span><%= plants[0].radio_saltTolerance %></li>
          </ul> 

          <ul> 
              <p class = "hd">Requirements</p>
             <li id = "sol-req"> <span>soil Requirements: </span><%= plants[0].soil %> </li> 
             <span></span>  <li id = "wt-req"> <span>Water Requirements: </span><%= plants[0].water %></li> 
               <li id = "tt-req"><span>Light Requirements: </span><%= plants[0].light %></li>
          </ul> 


          <ul>
              <p class = "hd">Management</p> 
                 <li id = "toxic"><span>Toxicity: </span><%= plants[0].radio_plantToxicity %></li> 
              <li id = "invade" > <span>Invasive Potential: </span><%= plants[0].radio_invasivePotential %></li> 
                <li id = "pests"><span>Susceptibility to Pests and Diseases: </span><%= plants[0].radio_suceptibilityToDiseases %></li> 
    
               <li id = "fr-lv"><span>Fruit/ Leaves/ Flowers litter: </span><%= plants[0].radio_fruitLeavesFlowerLitter %></li>
              <li id = "root"> <span>Surface Rooting: </span><%= plants[0].radio_surfaceRooting %> </li>
                <li id = "life"><span>Life Span: 25-50 years: </span><%= plants[0].radio_lifeSpan %></li> 
               <li id = "edible"><span>Edible Parts: </span><%= plants[0].radio_ediblePlantParts %></li>

          </ul>


        </div>
        




